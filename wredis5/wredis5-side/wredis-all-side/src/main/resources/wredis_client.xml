    <!--app下的name属性，对应示例代码中的appName值
    这样，程序就会根据用户指定的appName找到对应的key，然后进行初始化
    解决了线下环境和线上环境切换需要修改代码的问题
    如此，环境切换，只需修改key值即可-->
    <app key="XXX" name="wreidsDemoName">
        <connectionTimeout>3000</connectionTimeout>
        <soTimeout>3000</soTimeout>
        <maxRedirections>3</maxRedirections>
        <isReadWriteSeparate>false</isReadWriteSeparate>
        <clientName></clientName>
        <poolConfig>
            <maxTotal>256</maxTotal>
            <minIdle>32</minIdle>
            <blockWhenExhausted>true</blockWhenExhausted>
            <maxWaitMillis>1000</maxWaitMillis>
            <minEvictableIdleTimeMillis>30000</minEvictableIdleTimeMillis>
            <numTestsPerEvictionRun>30</numTestsPerEvictionRun>
            <testWhileIdle>true</testWhileIdle>
            <timeBetweenEvictionRunsMillis>600000</timeBetweenEvictionRunsMillis>
        </poolConfig>
    </app>
